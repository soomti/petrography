<%= form_for(resource, as: resource_name, url: session_path(resource_name),id: "project-contact-form") do |f| %>
  <div class="field">
    <%= f.label :email %><br />
    <%= f.email_field :email, autofocus: true, autocomplete: "email", class: "big-input", placeholder: "email" %>
  </div>
  <div class="field">
    <%= f.label :password %><br />
    <%= f.password_field :password, autocomplete: "current-password", class: "big-input", placeholder: "password" %>
  </div>
  <div class="actions">
    <%= f.submit "Log in",class: "btn btn-black btn-extra-large width-100 md-padding-15px-bottom sm-display-table sm-margin-lr-auto",style: "width:100%;" %>
  </div>
<% end %>